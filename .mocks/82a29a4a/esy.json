{
  "name": "@_linux.musl.x86_64/generate",
  "dependencies": {
    "@_linux.musl.x86_64/_opam_dune": "*",
    "@_linux.musl.x86_64/_opam_hex": "*",
    "@_linux.musl.x86_64/_opam_lwt": "*",
    "@_linux.musl.x86_64/_opam_mirage_crypto": "*",
    "@_linux.musl.x86_64/_opam_ppx_deriving": "*",
    "@_linux.musl.x86_64/_opam_ppx_deriving_yojson": "*",
    "@_linux.musl.x86_64/_opam_re": "*",
    "@_linux.musl.x86_64/_opam_reason": "*",
    "@_linux.musl.x86_64/ocaml": "*",
    "@_linux.musl.x86_64/sysroot": "*",
    "generate": "*",
    "generate-installer": "*"
  },
  "esy": {
    "buildsInSource": true,
    "buildEnv": {
      "DUNE_BUILD_DIR": "#{self.target_dir}",
      "cur__etc": "#{self.install}/linux.musl.x86_64-sysroot/etc",
      "cur__share": "#{self.install}/linux.musl.x86_64-sysroot/share",
      "cur__toplevel": "#{self.install}/linux.musl.x86_64-sysroot/toplevel",
      "cur__stublibs": "#{self.install}/linux.musl.x86_64-sysroot/stublibs",
      "cur__doc": "#{self.install}/linux.musl.x86_64-sysroot/doc",
      "cur__man": "#{self.install}/linux.musl.x86_64-sysroot/man",
      "cur__lib": "#{self.install}/linux.musl.x86_64-sysroot/lib",
      "cur__sbin": "#{self.install}/linux.musl.x86_64-sysroot/sbin",
      "cur__bin": "#{self.install}/linux.musl.x86_64-sysroot/bin",
      "cur__install": "#{self.install}/linux.musl.x86_64-sysroot",
      "cur__original_root": "#{self.original_root}",
      "OCAMLFIND_CONF": "%{store}%/findlib/#{self.id}/findlib.conf",
      "OCAMLFIND_DESTDIR": "#{self.install}/linux.musl.x86_64-sysroot/lib",
      "TOPKG_CONF_TOOLCHAIN": "linux.musl.x86_64",
      "OCAMLLIB": null,
      "OCAMLPATH": null
    },
    "exportedEnv": {
      "ESY__GENERATE_INSTALLER": {
        "scope": "global",
        "val": "#{self.original_root}/generate-installer"
      },
      "ESY__GENERATE_PATCHES": {
        "scope": "global",
        "val": "#{self.original_root}/patches"
      },
      "ESY__GENERATE_SYSROOT": {
        "scope": "global",
        "val": "#{self.original_root}/sysroot"
      }
    },
    "build": [
      [
        "cp", "-r", "-p", "/Users/Jaap/.esy/source/i/generate__14300125/.",
        "#{self.root}"
      ],
      [ "mv", "#{self.install}", "_____install_____" ],
      [ "mkdir", "-p", "#{self.install}/_esy" ],
      [
        "mv", "_____install_____",
        "#{self.install}/linux.musl.x86_64-sysroot"
      ],
      [
        "dune", "build", "-p", "generate", "-x", "linux.musl.x86_64",
        "@install"
      ]
    ],
    "install": [ [ "generate-installer", "linux.musl.x86_64" ] ]
  }
}