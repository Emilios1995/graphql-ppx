    "buildEnv": {
      "cur__etc": "#{self.install}/linux.musl.x86_64-sysroot/etc",
      "cur__share": "#{self.install}/linux.musl.x86_64-sysroot/share",
      "cur__toplevel": "#{self.install}/linux.musl.x86_64-sysroot/toplevel",
