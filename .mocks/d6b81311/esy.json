      [
        "mv", "_____install_____",
        "#{self.install}/linux.musl.x86_64-sysroot"
      ],
      [
        "dune", "exec", "-p", "lwt", "src/unix/config/discover.exe", "--",
        "--save", "--use-libev", "false"
      ],
      [
        "dune", "build", "-p", "lwt", "-j", "4", "-x", "linux.musl.x86_64",
        "@install"
      ]
    ],
    "install": [ [ "generate-installer", "linux.musl.x86_64" ] ]
