{
  "name": "graphql_ppx",
  "version": "0.1.0",
  "description": "Reason workflow with Esy",
  "license": "MIT",
  "esy": {
    "build": "refmterr dune build -p graphql_ppx",
    "NOTE": "Optional release Section. Customizes result of `esy release`",
    "release": {
      "releasedBinaries": [
        "graphql_ppx.exe"
      ],
      "deleteFromBinaryRelease": [
        "*esy_ocaml_*",
        "*refmterr-*",
        "ocaml-*"
      ]
    }
  },
  "dependencies": {
    "@opam/dune": "*",
    "@opam/result": "*",
    "@opam/yojson": "*",
    "@opam/ocaml-migrate-parsetree": "*",
    "@opam/ppx_tools_versioned": "*",
    "@esy-ocaml/reason": "*",
    "refmterr": "*",
    "ocaml": "~4.2.3"
  },
  "resolutions": {
    "@esy-ocaml/esy-installer": "0.0.1",
    "@opam/menhir": "20171013",
    "@opam/re": "1.8.0"
  },
  "devDependencies": {
    "@esy-ocaml/merlin": "*",
    "ocaml": "~4.2.3"
  },
  "peerDependencies": {
    "ocaml": " >= 4.2.1  < 4.7.0"
  }
}
